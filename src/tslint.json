{
  "extends": "../tslint.json",
  "semicolon": [
    false
  ],
  "quotemark": [
    true,
    "double"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
