{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-ekwg": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:pipe": {
          "lintFix": true
        },
        "@schematics/angular:class": {
          "lintFix": true
        },
        "@schematics/angular:module": {
          "lintFix": true
        },
        "@schematics/angular:service": {
          "lintFix": true
        },
        "@schematics/angular:component": {
          "lintFix": true,
          "prefix": "app",
          "styleext": "sass",
          "style": "sass"
        },
        "@schematics/angular:directive": {
          "lintFix": true,
          "prefix": "app"
        }
      },
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/ng-ekwg",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "input": "src/legacy/app/assets",
                "glob": "**/**/*.*",
                "output": "/assets"
              },
              {
                "input": "src/legacy/app/partials",
                "glob": "**/**/*.*",
                "output": "/partials"
              }
            ],
            "styles": [
              "src/legacy/vendor/bootstrap/dist/css/bootstrap.min.css",
              "src/legacy/vendor/font-awesome/css/font-awesome.min.css",
              "src/legacy/vendor/angular-ui-select/dist/select.min.css",
              "src/legacy/app/assets/css/style.css",
              "src/app/styles/bs-datepicker.css",
              "src/app/styles/ui-switch.component.scss",
              "src/styles.sass"
            ],
            "scripts": [
              "./node_modules/marked/lib/marked.js",
              "src/legacy/vendor/jquery/dist/jquery.slim.js",
              "node_modules/angular/angular.min.js",
              "node_modules/angular-resource/angular-resource.min.js",
              "node_modules/angular-route/angular-route.min.js",
              "node_modules/angular-sanitize/angular-sanitize.min.js",
              "node_modules/angular-cookies/angular-cookies.min.js",
              "node_modules/angular-animate/angular-animate.min.js",
              "node_modules/angular-logger/dist/angular-logger.min.js",
              "src/legacy/vendor/angularjs-mongolab-promise/mongolabResourceHttp.js",
              "src/legacy/vendor/angular-bootstrap/ui-bootstrap-tpls.min.js",
              "src/legacy/vendor/angular-ui-select/dist/select.min.js",
              "src/legacy/vendor/angular-modal-service/dst/angular-modal-service.js",
              "src/legacy/vendor/angular-easyfb/build/angular-easyfb.min.js",
              "src/legacy/vendor/ng-file-upload/ng-file-upload.min.js",
              "src/legacy/vendor/ng-csv/build/ng-csv.min.js",
              "src/legacy/vendor/xml2json/xml2json.js",
              "src/legacy/vendor/showdown/dist/showdown.min.js",
              "src/legacy/vendor/angular-markdown-directive/markdown.js",
              "src/legacy/vendor/bootstrap/dist/js/bootstrap.min.js",
              "src/legacy/vendor/underscore/underscore-min.js",
              "src/legacy/vendor/underscore.string/dist/underscore.string.min.js",
              "src/ekwg-legacy/app/js/ng-ekwg.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "ng-ekwg:build",
            "proxyConfig": "proxy.config.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-ekwg:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-ekwg:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.sass"
            ],
            "scripts": [
              "./node_modules/marked/lib/marked.js",
              "src/legacy/vendor/jquery/dist/jquery.slim.js",
              "node_modules/angular/angular.min.js",
              "node_modules/angular-resource/angular-resource.min.js",
              "node_modules/angular-route/angular-route.min.js",
              "node_modules/angular-sanitize/angular-sanitize.min.js",
              "node_modules/angular-cookies/angular-cookies.min.js",
              "node_modules/angular-animate/angular-animate.min.js",
              "node_modules/angular-logger/dist/angular-logger.min.js",
              "src/legacy/vendor/angularjs-mongolab-promise/mongolabResourceHttp.js",
              "src/legacy/vendor/angular-bootstrap/ui-bootstrap-tpls.min.js",
              "src/legacy/vendor/angular-ui-select/dist/select.min.js",
              "src/legacy/vendor/angular-modal-service/dst/angular-modal-service.js",
              "src/legacy/vendor/angular-easyfb/build/angular-easyfb.min.js",
              "src/legacy/vendor/ng-file-upload/ng-file-upload.min.js",
              "src/legacy/vendor/ng-csv/build/ng-csv.min.js",
              "src/legacy/vendor/xml2json/xml2json.js",
              "src/legacy/vendor/showdown/dist/showdown.min.js",
              "src/legacy/vendor/angular-markdown-directive/markdown.js",
              "src/legacy/vendor/bootstrap/dist/js/bootstrap.min.js",
              "src/legacy/vendor/underscore/underscore-min.js",
              "src/legacy/vendor/underscore.string/dist/underscore.string.min.js",
              "src/ekwg-legacy/app/js/ng-ekwg.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-ekwg-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-ekwg:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-ekwg:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-ekwg",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "sass"
    }
  }
}
