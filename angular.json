{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-ekwg": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:pipe": {
          "lintFix": true
        },
        "@schematics/angular:class": {
          "lintFix": true
        },
        "@schematics/angular:module": {
          "lintFix": true
        },
        "@schematics/angular:service": {
          "lintFix": true
        },
        "@schematics/angular:component": {
          "lintFix": true,
          "prefix": "app",
          "styleext": "sass",
          "style": "sass"
        },
        "@schematics/angular:directive": {
          "lintFix": true,
          "prefix": "app"
        }
      },
      "architect": {
        "build": {
          "builder": "angular-builder-custom-terser-options:browser-custom-terser",
          "options": {
            "outputPath": "dist/ng-ekwg",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "input": "src/ekwg-legacy",
                "glob": "**/*",
                "output": "/"
              },
              {
                "input": "node_modules/angular-logger/dist",
                "glob": "**/angular-logger.min.js",
                "output": "/vendor/angular-logger"
              },
              {
                "input": "src/legacy/vendor/angular-easyfb/build",
                "glob": "**/angular-easyfb.min.js",
                "output": "/vendor/angular-easyfb"
              },
              {
                "input": "src/legacy/vendor/angularjs-mongolab-promise",
                "glob": "**/mongolabResourceHttp.js",
                "output": "/vendor/angularjs-mongolab-promise"
              },
              {
                "input": "src/legacy/vendor/angular-bootstrap",
                "glob": "**/ui-bootstrap-tpls.min.js",
                "output": "/vendor/angular-bootstrap"
              },
              {
                "input": "src/legacy/vendor/angular-modal-service/dst",
                "glob": "**/angular-modal-service.*",
                "output": "/vendor/angular-modal-service"
              },
              {
                "input": "src/legacy/vendor/angular-ui-select/dist",
                "glob": "**/select*",
                "output": "/vendor/angular-ui-select"
              },
              {
                "input": "src/legacy/vendor/ng-file-upload",
                "glob": "**/ng-file-upload.min.js",
                "output": "/vendor/ng-file-upload"
              },
              {
                "input": "src/legacy/vendor/ng-csv/build",
                "glob": "**/ng-csv.min.js",
                "output": "/vendor/ng-csv"
              },
              {
                "input": "src/legacy/vendor/angular",
                "glob": "**/angular.min.js.map",
                "output": "/vendor/angular"
              },
              {
                "input": "src/legacy/vendor/angular-cookies",
                "glob": "**/angular-cookies.min.js.map",
                "output": "/vendor/angular"
              },
              {
                "input": "src/legacy/vendor/bootstrap/dist",
                "glob": "**/**/*.*",
                "output": "/vendor/bootstrap/dist"
              },
              {
                "input": "src/legacy/vendor/font-awesome/css",
                "glob": "**/**/*.*",
                "output": "/vendor/font-awesome/css"
              },
              {
                "input": "src/legacy/vendor/font-awesome/fonts",
                "glob": "**/**/*.*",
                "output": "/vendor/font-awesome/fonts"
              },
              {
                "input": "src/legacy/vendor/font-awesome/less",
                "glob": "**/**/*.*",
                "output": "/vendor/font-awesome/less"
              },
              {
                "input": "src/legacy/vendor/font-awesome/scss",
                "glob": "**/**/*.*",
                "output": "/vendor/font-awesome/scss"
              },
              {
                "input": "src/legacy/vendor/html5boilerplate",
                "glob": "**/**/*.css",
                "output": "/vendor/html5boilerplate"
              },
              {
                "input": "src/legacy/vendor/jquery/dist",
                "glob": "**/jquery.slim.min.*",
                "output": "/vendor/jquery"
              },
              {
                "input": "src/legacy/vendor/json3",
                "glob": "**/json3.min.js",
                "output": "/vendor/json3"
              },
              {
                "input": "src/legacy/vendor/es5-shim",
                "glob": "**/**/*.*",
                "output": "/vendor/es5-shim"
              },
              {
                "input": "src/legacy/vendor/underscore",
                "glob": "**/**/*underscore-min.*",
                "output": "/vendor/underscore"
              },
              {
                "input": "src/legacy/vendor/underscore.string/dist",
                "glob": "**/underscore.string.min.js",
                "output": "/vendor/underscore-string"
              },
              {
                "input": "src/legacy/vendor/showdown/dist",
                "glob": "**/showdown.min.js*",
                "output": "/vendor/showdown"
              },
              {
                "input": "src/legacy/vendor/angular-markdown-directive",
                "glob": "**/markdown.js",
                "output": "/vendor/angular-markdown-directive"
              },
              {
                "input": "src/legacy/vendor/xml2json",
                "glob": "**/**/*.*",
                "output": "/vendor/xml2json"
              },
              {
                "input": "src/legacy/app/assets",
                "glob": "**/**/*.*",
                "output": "/assets"
              },
              {
                "input": "src/legacy/app/partials",
                "glob": "**/**/*.*",
                "output": "/partials"
              }
            ],
            "styles": [
              "src/app/styles/bs-datepicker.css",
              "src/app/styles/ui-switch.component.scss",
              "src/styles.sass"
            ],
            "scripts": [
              "./node_modules/marked/lib/marked.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "terserOptions": {
                "keep_fnames": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "angular-builder-custom-terser-options:dev-server-custom-terser",
          "options": {
            "browserTarget": "ng-ekwg:build",
            "proxyConfig": "proxy.config.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-ekwg:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-ekwg:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.sass"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-ekwg-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-ekwg:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-ekwg:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-ekwg",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "sass"
    }
  }
}
