    "@types/lodash-es": "4.17.3",
    "@types/node": "10.14.20",
    "@types/sinon": "4.3.3",
    "@types/sinon-chai": "3.2.2",
    "@types/underscore": "1.8.1",
    "@types/underscore.string": "0.0.30",
    "angular-logger": "1.3.2",
    "aws-sdk": "2.404.0",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "chai": "4.2.0",
    "cheerio": "0.16.0",
    "chromedriver": "84.0.1",
    "compression": "1.7.4",
